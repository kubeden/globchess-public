<body class="w-full text-center justify-center items-center h-screen bg-neutral-950">

    <!-- this is the registraion page -->


    <div class="flex flex-col h-full items-center justify-center">
        <img src="assets/img/logo.png" class="" width="100" alt="">
        <h2 class="text-white text-4xl md:text-5xl font-black tracking-widest pt-4 pb-4">GLOBCHESS</h2>
        <!-- <p class="text-white italic text-xl md:text-xl pb-6 pt-2 md:py-4">World's largest chess game</p> -->
        <div id="registerDiv" class="pb-8 flex flex-col">
            <!-- email password for register -->
            <form id="registerForm" class="flex flex-col" method="POST" action="/register">
                <input type="email" id="email" class="bg-neutral-300 px-4 py-2 text-black font-bold rounded-md text-base w-80 focus:bg-white" placeholder="Email">
                <input type="password" id="password" class="mt-1 bg-neutral-300 px-4 py-2 text-black font-bold rounded-md text-base w-80 focus:bg-white" placeholder="Password">
                <input type="confirmPassword" id="confirmPassword" class="mt-1 bg-neutral-300 px-4 py-2 text-black font-bold rounded-md text-base w-80 focus:bg-white" placeholder="Confirm Password *">
                
                <!-- button for register -->
                <button id="registerBtn" class="bg-green-600 text-white font-bold rounded-md text-base w-80 py-2 mt-4">Register Now</button>
            </form>
            
            
            <span class="text-sm italic text-neutral-600 pt-3">Already have an account? <a href="/" class="text-neutral-500">Login Now</a></span>
        </div>
    </div>




